let nomes = ["Joao", "Pedro", "Lucas", "Daniele", "Marcos"];

console.log(nomes[2]);

nomes.push("Fernanda");
nomes.unshift("Alex");

nomes.pop();
console.log(nomes);

let numeros = [2, 4, 6, 8];
let numerosDobrados = numeros.map(num => num * 2);
console.log(numerosDobrados);

let numeros2 = [1, 3, 5, 7, 9];
let maioresQue5 = numeros2.filter(num => num > 5);
console.log(maioresQue5);
